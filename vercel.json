{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/tasks",
      "dest": "app.js"
    },
    {
      "src":"/api/v1/tasks/(.*)",
      "headers":{"Location":"/api/v1/tasks/$1"},
      "dest": "app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/404.html"
    }
  ]
}