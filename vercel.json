{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/tasks",
      "dest": "app.js"
    },
    {
      "src": "/api/v1/tasks/(.*)",
      "headers": { "Location": "/api/v1/tasks/$1" },
      "dest": "app.js"
    },
    {
      "src": "/api/v1/users",
      "dest": "app.js"
    },
    {
      "src": "/api/v1/users/(.*)",
      "headers": { "Location": "/api/v1/users/$1" },
      "dest": "app.js"
    },
    {
      "src": "/api/v1/users/(.*)/tasks",
      "headers": { "Location": "/api/v1/users/$1/tasks" },
      "dest": "app.js"
    },
    {
      "src": "/api/v1/register",
      "dest": "app.js"
    },
    {
      "src": "/api/v1/login",
      "dest": "app.js"
    },
    {
      "src": "/api/v1/verify-token",
      "dest": "app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/404.html"
    }
  ]
}